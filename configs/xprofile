#!/usr/bin/env bash
export NO_AT_BRIDGE=1
/usr/bin/wmname LG3D &
udiskie --tray &
xmodmap ~/.Xmodmap
xset r rate 250 100 &
setxkbmap -option ctrl:nocaps
synclient MaxTapTime=0
feh --bg-fill .i3/deep-dark-space.png
syndaemon -i 0.2 -K -R &
xss-lock -- i3lock -n -i ~/.i3/galaxy.png &
flameshot &
mattermost-desktop &
xrandr --output eDP-1 --mode "2048x1152" &
solaar &
